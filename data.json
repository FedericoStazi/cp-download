{
    "folder": "~/Projects/competitive-programming/",
    "sources": {
        "codeforces": {
            "username": "FedericoS",
            "max_page": {
                "url": "https://codeforces.com/submissions/__username__",
                "js":{
                    "max_page": "$(\".page-index\").last().text()"
                }

            },
            "solved_problems": {
                "url": "",
                "js":{
                    "get_elements_number": "",
                    "id": ""
                }

            },
            "submissions_per_page": {
                "url": "https://codeforces.com/submissions/__username__/page/__page_id__",
                "page_one_url": "https://codeforces.com/submissions/__username__",
                "js": {
                    "get_elements_number": "$(\".id-cell\").length",
                    "id": "$(\".id-cell\").eq(__element__).children().eq(0).attr(\"href\")",
                    "name": "$(\"[data-submission-id]\").eq(__element__).children().eq(3).children().eq(0).attr(\"href\").replace(\"/contest/\",\"\").replace(\"/problem/\",\"\")",
                    "time": "(new Date($(\"[data-submission-id]\").eq(__element__).children().eq(1).text().split(\"UTC\")[0].trim())).getTime()",
                    "solved": "$(\".status-verdict-cell\").eq(__element__).text().trim() == \"Accepted\""
                }
            },
            "code":{
                "url": "https://codeforces.com__problem_id__",
                "js":{
                    "lines_number": "$(\"#program-source-text\").find(\"li\").length",
                    "line": "$(\"#program-source-text\").find(\"li\").eq(__element__).text()",
                    "extension": "\".cpp\""
                }
            }
        },
        "codechef": {
            "username": "federico_s",
            "max_page": {
                "url": "",
                "js":{
                    "max_page": "1"
                }

            },
            "solved_problems": {
                "url": "https://www.codechef.com/users/__username__",
                "js":{
                    "get_elements_number": "$(\"h5\").eq(0).text().slice(14,-1)",
                    "id": "$(\"article\").eq(0).children().children().children().eq(__element__).attr(\"href\")"
                }

            },
            "submissions_per_page": {
                "url": "https://www.codechef.com__page_id__",
                "page_one_url": "",
                "js": {
                    "get_elements_number": "$(\"tbody\").eq(2).children().length",
                    "id": "$(\"tbody\").eq(2).children().eq(__element__).children().eq(0).text()",
                    "name": "$(\"h2\").eq(1).children().eq(1).text()",
                    "time": "0",
                    "solved": "$(\"tbody\").eq(2).children().eq(__element__).children().eq(3).children().eq(0).children().eq(0).attr(\"src\") == \"/misc/tick-icon.gif\""
                }
            },
            "code":{
                "url": "https://www.codechef.com/viewsolution/__problem_id__",
                "js":{
                    "lines_number": "$(\".ace_line\").length",
                    "line": "$(\".ace_line\").eq(__element__).text()",
                    "extension": "\".cpp\""
                }
            }
        }
    }
}
